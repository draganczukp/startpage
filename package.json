{
	"name": "full-page-panels",
	"version": "0.8.0",
	"description": "A full page panels with scrolling support",
	"main": "dist/js/Scroller.js",
	"repository": "https://github.com/draganczukp/pane-website",
	"author": "Przemek Draga≈Ñczuk",
	"license": "MIT",
	"private": false,
	"scripts": {
		"dev": "npm-run-all --parallel live watch-sass watch-ts",
		"live": "live-server --wait=600",
		"watch-sass": "sass --watch src/scss:dist/css",
		"watch-ts": "tsc -w",
		"build": "sass src/scss:dist/css && tsc"
	},
	"devDependencies": {
		"live-server": "^1.2.0",
		"npm-run-all": "^4.1.3",
		"sass": "^1.15.1",
		"typescript": "^3.1.6"
	}
}
